{
  "name": "Secure password delivery from clipboard",
  "description": "Inserts encoded passwords into password fields.",
  "version": "0.2",
  "permissions": ["contextMenus", "clipboardRead", "clipboardWrite", "activeTab"],
  "background": {
    "persistent": false,
    "scripts": ["program.js"]
  },
  "icons" : {
    "48" : "icon-48x48.png",
    "128" : "icon-128x128.png"
  },
  "manifest_version": 2
}
